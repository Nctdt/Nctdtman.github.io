  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/websocket": "^1.0.5",
    "@vitejs/plugin-react": "^1.3.0",
    "esno": "^0.16.3",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
